all:
	cd compare && make compile && make link
	cd executer && make compile && make link
	cd sequencer && make compile && make link

debug:
	cd compare && make compile_debug && make link
	cd executer && make compile_debug && make link
	cd sequencer && make compile_debug && make link
	
clean:
	cd compare && make clean
	cd executer && make clean
	cd sequencer && make clean

